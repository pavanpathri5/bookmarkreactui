Close={this.closeModal}\n          contentLabel=\"Form Modal\"\n          className=\"ReactModal__Content\" // Apply the modal content class\n          overlayClassName=\"ReactModal__Overlay\" // Apply the overlay class\n        >\n          <h2 className=\"modal-title\">Form Popup</h2>\n          <form className=\"modal-form\" onSubmit={this.handleSubmit}>\n            <div>\n              <label htmlFor=\"firstName\">BookMark Name</label>\n              <input\n                type=\"text\"\n                id=\"name\"\n                name=\"name\"\n                value={formData.name}\n                onChange={this.handleInputChange}\n              />\n            </div>\n            <div>\n              <label htmlFor=\"lastName\">Description:</label>\n              <input\n                type=\"text\"\n                id=\"description\"\n                name=\"description\"\n                value={formData.description}\n                onChange={this.handleInputChange}\n              />\n            </div>\n\n            <div>\n              <label htmlFor=\"email\">Link/Server:</label>\n              <input\n                type=\"text\"\n                id=\"link\"\n                name=\"link\"\n                value={formData.link}\n                onChange={this.handleInputChange}\n              />\n            </div>\n\n            <div>\n              <label htmlFor=\"email\">User Name:</label>\n              <input\n                type=\"text\"\n                id=\"user\"\n                name=\"user\"\n                value={formData.user}\n                onChange={this.handleInputChange}\n              />\n            </div>\n            \n            <div>\n              <label htmlFor=\"email\">Type :</label>\n              <input\n                type=\"text\"\n                id=\"tag\"\n                name=\"tag\"\n                value={formData.tag}\n                onChange={this.handleInputChange}\n              />\n            </div>\n            <button type=\"submit\">Submit</button>\n          </form>\n        </Modal>\n      </div>\n    );\n  }\n}\n\nexport default CreateForm;\n","/Users/pavankumarpathri/Documents/bookmarkreactui/src/SearchBar.js",[],[],{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","severity":1,"message":"43","line":1,"column":8,"nodeType":"44","messageId":"45","endLine":1,"endColumn":12},{"ruleId":"42","severity":1,"message":"46","line":61,"column":19,"nodeType":"44","messageId":"45","endLine":61,"endColumn":30},{"ruleId":"42","severity":1,"message":"47","line":61,"column":32,"nodeType":"44","messageId":"45","endLine":61,"endColumn":44},{"ruleId":"48","severity":1,"message":"49","line":84,"column":21,"nodeType":"50","messageId":"51","endLine":84,"endColumn":91,"fix":"52"},{"ruleId":"42","severity":1,"message":"53","line":58,"column":13,"nodeType":"44","messageId":"45","endLine":58,"endColumn":24},"no-new-object",["54"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'currentPage' is assigned a value but never used.","'itemsPerPage' is assigned a value but never used.","react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" (which implies rel=\"noopener\") is a security risk in older browsers: see https://mathiasbynens.github.io/rel-noopener/#recommendations","JSXOpeningElement","noTargetBlankWithoutNoreferrer",{"range":"55","text":"56"},"'isModalOpen' is assigned a value but never used.","no-object-constructor",[2431,2431]," rel=\"noreferrer\""]